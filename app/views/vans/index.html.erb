<div class="container">
  <div class="row">
    <div class="col-xs-12 col-sm-10 col-sm-offset-1">
      <h1>All vans</h1>
      <ul class="list-group mb-3">
        <% @vans.each do |van| %>
          <li class="list-group-item">
            <%= link_to van.model, van_path(van) %>
          </li>
        <% end %>
      </ul>
      <%# <%= link_to "Add a new van", new_van_path, class: 'btn btn-primary' %>
    </div>
  </div>
</div>
